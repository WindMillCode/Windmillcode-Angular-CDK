{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noPropertyAccessFromIndexSignature": false,
    "allowJs": false,
    "noImplicitAny": false,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "moduleResolution": "bundler",
    "importHelpers": true,
    "target": "ES2022",
    "module": "ES2022",
    "allowSyntheticDefaultImports": false,
    "resolveJsonModule": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "lib": [
      "ES2022",
      "dom"
    ],
    "paths": {
      "@angular/*": [
        "node_modules/@angular/*"
      ],
      "@windmillcode/angular-wml-accordion": [
        "dist/wml-accordion"
      ],
      "@windmillcode/angular-wml-button": [
        "dist/wml-button"
      ],
      "@windmillcode/angular-wml-carousel": [
        "dist/wml-carousel"
      ],
      "@windmillcode/angular-wml-carousel-one": [
        "dist/wml-carousel-one"
      ],
      "@windmillcode/angular-wml-chips": [
        "dist/wml-chips"
      ],
      "@windmillcode/angular-wml-components-base": [
        "dist/wml-angular-components-base"
      ],
      "@windmillcode/angular-wml-field": [
        "dist/wml-field"
      ],
      "@windmillcode/angular-wml-form": [
        "dist/wml-form"
      ],
      "@windmillcode/angular-wml-infinite-dropdown": [
        "dist/wml-infinite-dropdown"
      ],
      "@windmillcode/angular-wml-input": [
        "dist/wml-input"
      ],
      "@windmillcode/angular-wml-label": [
        "dist/wml-label"
      ],
      "@windmillcode/angular-wml-options": [
        "dist/wml-options"
      ],
      "@windmillcode/angular-wml-popup": [
        "dist/wml-popup"
      ],
      "@windmillcode/angular-wml-schematics": [
        "dist/wml-schematics"
      ],
      "@windmillcode/angular-wml-select": [
        "dist/wml-select"
      ],
      "@windmillcode/wml-components-base": [
        "dist/wml-components-base"
      ]
    },
    "types": null,
    "useDefineForClassFields": false
  },
  "exclude": null,
  "angularCompilerOptions": {
    "enableI18nLegacyMessageIdFormat": false,
    "strictInjectionParameters": true,
    "strictInputAccessModifiers": true,
    "strictTemplates": true
  }
}